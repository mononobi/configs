# In how many days should the root CA key expire?
set_var EASYRSA_CA_EXPIRE      7320
# In how many days should certificates expire?
set_var EASYRSA_CERT_EXPIRE    5490
set_var EASYRSA_REQ_COUNTRY    "DE"
set_var EASYRSA_REQ_PROVINCE   "Hamburg"
set_var EASYRSA_REQ_CITY       "Hamburg"
set_var EASYRSA_REQ_ORG        "Private"
set_var EASYRSA_REQ_EMAIL      "your-email@somewhere.com"
set_var EASYRSA_REQ_OU         "Community"
set_var EASYRSA_ALGO "ec"
set_var EASYRSA_DIGEST "sha512"
